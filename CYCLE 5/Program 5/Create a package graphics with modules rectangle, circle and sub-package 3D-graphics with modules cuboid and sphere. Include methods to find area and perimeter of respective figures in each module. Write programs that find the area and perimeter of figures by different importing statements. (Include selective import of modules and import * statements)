#selective import
from graphics.rectangle import area as rect_area,perimeter as rect_perimeter
from graphics.circle import area as circ_area,perimeter as circ_perimeter
from graphics.three_d_graphics.cuboid import area as cuboid_area,perimeter as cuboid_perimeter
from graphics.three_d_graphics.sphere import area as sphere_area,perimeter as sphere_perimeter
print("Rectangle Area:",rect_area(5,3))
print("Rectangle Perimeter:",rect_perimeter(5,3))
print("Circle Area:",circ_area(4))
print("Circle Perimeter:",circ_perimeter(4))
print("Cuboid Area:",cuboid_area(2,3,4))
print("Cuboid Perimeter:",cuboid_perimeter(2,3,4))
print("Sphere Area:",sphere_area(6))
print("Sphere Perimeter:",sphere_perimeter(6))
